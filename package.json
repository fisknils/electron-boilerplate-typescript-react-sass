{
  "name": "electron-react-boilerplate",
  "version": "1.0.0",
  "description": "A simple Electron application using TypeScript, React and Sass",
  "scripts": {
    "dev": "rimraf dist && electron-webpack dev",
    "prod": "rimraf dist && electron-webpack prod",
    "dist": "npm run prod && electron-builder",
    "dist:dir": "npm run dist --dir -c.compression=store -c.mac.identity=null"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Chuzzy/electron-react-typescript.git"
  },
  "keywords": [
    "electron",
    "react",
    "typescript",
    "babel",
    "webpack",
    "sass"
  ],
  "author": "Chuzzy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Chuzzy/electron-react-typescript/issues"
  },
  "main": "dist/main/index.js",
  "homepage": "https://github.com/Chuzzy/electron-react-typescript#readme",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/electron": "^1.6.10",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.2",
    "electron": "^9.2.0",
    "electron-builder": "^22.8.0",
    "electron-webpack": "^2.8.2",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {}
}
